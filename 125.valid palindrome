class Solution {
    public boolean isPalindrome(String s) {
        String s1=s.replaceAll("[^a-zA-Z0-9]","").toLowerCase();
        StringBuilder k=new StringBuilder(s1);
        String re=k.reverse().toString();
        if(s1.equals(re))
        {
            return true;
        }
        else
        {
            return false;
        }

        
    }
}
